{
    "admin": {
        "realm": "keycloak-admin"
    },

    "audit": {
        "provider": "${keycloak.audit.provider:jpa}"
    },

    "model": {
        "provider": "${keycloak.model.provider:jpa}"
    },

    "timer": {
        "provider": "basic"
    },

    "theme": {
        "default": "keycloak",
        "folder": {
            "dir": "${keycloak.theme.dir}"
        }
    },

    "login-forms": {
        "provider": "freemarker"
    },

    "account": {
        "provider": "freemarker"
    },

    "email": {
        "provider": "freemarker"
    },

    "scheduled": {
        "interval": 900
    }
}