{
    "admin": {
        "realm": "master"
    },

    "model": {
        "provider": "jpa"
    },

    "modelCache": {
        "provider": "${keycloak.model.cache.provider:}"
    },

    "timer": {
        "provider": "basic"
    },

    "theme": {
        "default": "keycloak",
        "folder": {
            "dir": "${jboss.server.config.dir}/themes"
        }
    },

    "scheduled": {
        "interval": 900
    }
}